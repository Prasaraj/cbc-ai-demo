{
    "functions": {
      "api/index.py": {
        "maxDuration": 45,
        "memory": 1024
      }
    },
    "builds": [
      {
        "src": "api/index.py",
        "use": "@vercel/python"
      },
      {
        "src": "streamlit_app.py",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "static"
        }
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "api/index.py"
      },
      {
        "src": "/(.*)",
        "dest": "/streamlit_app.py"
      }
    ]
  }